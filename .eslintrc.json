{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        // no underscore
        "no-underscore-dangle": "off"
    },
    "ignorePatterns":[
        "node_modules/*",
        "dist/*",
        "postcss.config.js",
        "webpack.*.js",
        "tailwind.config.js"
    ]
}
